{
  "name": "react-state-management",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "hoisted": "artillery run -o tests/hoisted.json tests/hoisted.yml",
    "reducer": "artillery run -o tests/reducer.json tests/reducer.yml",
    "custom-hook": "artillery run -o tests/custom-hook.json tests/custom-hook.yml",
    "context-provider": "artillery run -o tests/context-provider.json tests/context-provider.yml",
    "mobx": "artillery run -o tests/mobx.json tests/mobx.yml",
    "redux": "artillery run -o tests/redux.json tests/redux.yml",
    "recoil": "artillery run -o tests/recoil.json tests/recoil.yml",
    "perf:artillery": "run-s hoisted reducer custom-hook context-provider mobx redux recoil",
    "playwright:test": "playwright test",
    "stats": "nodemon -w e2e -w src/charts/stats.js src/charts/stats",
    "enchilada": "run-p dev playwright:test stats",
    "gen": "playwright codegen http://localhost:5173"
  },
  "dependencies": {
    "@mdi/js": "^7.0.96",
    "@reduxjs/toolkit": "^1.8.6",
    "chart.js": "^3.9.1",
    "currency": "^4.1.0",
    "date-fns": "^2.29.3",
    "express": "^4.18.2",
    "lodash.merge": "^4.6.2",
    "mobx": "^6.6.2",
    "mobx-react-lite": "^3.4.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^4.3.1",
    "react-code-blocks": "0.0.9-0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "react-syntax-highlighter": "^15.5.0",
    "recoil": "^0.7.6"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@playwright/test": "^1.27.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@vitejs/plugin-react": "^2.1.0",
    "artillery": "2.0.0-23",
    "artillery-engine-playwright": "^0.2.0",
    "autoprefixer": "^10.4.12",
    "daisyui": "^2.33.0",
    "eslint": "^8.26.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.10",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.27.1",
    "playwright-watch": "^1.3.23",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.1",
    "vite": "^3.1.8",
    "vitest": "^0.24.3"
  }
}